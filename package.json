{
  "name": "shipeasy",
  "version": "0.0.1",
  "private": true,
  "description": "ShipEasy SaaS Template - A comprehensive monorepo for building subscription-based web applications",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "dev:next": "turbo run dev --filter=@shipeasy/next-app",
    "dev:nuxt": "turbo run dev --filter=@shipeasy/nuxt-app",
    "build:next": "turbo run build --filter=@shipeasy/next-app",
    "build:nuxt": "turbo run build --filter=@shipeasy/nuxt-app",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:check": "node libs/database/scripts.js check",
    "db:push": "node libs/database/scripts.js push",
    "db:generate": "node libs/database/scripts.js generate",
    "db:migrate": "node libs/database/scripts.js migrate",
    "db:seed": "node libs/database/scripts.js seed",
    "db:studio": "node libs/database/scripts.js studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "saas",
    "subscription",
    "next.js",
    "nuxt.js",
    "monorepo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@types/mjml": "4.7.4",
    "@types/node": "^20.10.5",
    "@types/pg": "8.15.4",
    "@vitest/ui": "3.0.9",
    "drizzle-kit": "0.30.5",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "10.1.1",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "8.27.0",
    "vitest": "3.0.9"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "0.2.8",
    "@ai-sdk/openai": "1.3.9",
    "@alicloud/dysmsapi20170525": "3.1.1",
    "@alicloud/openapi-client": "0.4.13",
    "@alicloud/tea-util": "1.4.10",
    "@casl/ability": "6.7.3",
    "@formatjs/intl-localematcher": "0.6.1",
    "@peculiar/x509": "1.12.3",
    "@types/chalk": "2.2.4",
    "@types/negotiator": "0.6.3",
    "ai": "4.3.4",
    "better-auth": "1.2.4",
    "chalk": "5.4.1",
    "creem": "0.3.37",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.1",
    "formidable": "1.2.6",
    "handlebars": "4.7.8",
    "mjml": "4.15.3",
    "negotiator": "1.0.0",
    "ofetch": "1.4.1",
    "pg": "^8.14.1",
    "resend": "4.1.2",
    "stripe": "18.1.0",
    "tsx": "^4.19.3",
    "twilio": "5.5.2",
    "validation-better-auth": "1.1.1",
    "wechatpay-node-v3": "2.2.1"
  }
}
